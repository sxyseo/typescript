{
  "name": "ts-demo",
  "version": "0.0.0",
  "description": "Typescript webpack demo",
  "author": {
    "name": "ar.insect",
    "email": "ar.insect@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "run-s clean:coverage clean:target",
    "clean:coverage": "del-cli coverage",
    "clean:target": "del-cli target",
    "heroku-postbuild": "npm run build",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js src server",
    "lint:ts": "tslint 'src/**/*.ts'",
    "postinstall": "npm run typings",
    "prebuild": "npm run clean:target",
    "pretest": "run-s clean:coverage lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run --coverage",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "typings": "typings install",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.20",
    "backbone": "^1.3.3",
    "compression": "~1.6.2",
    "d3": "^4.4.0",
    "express": "~4.14.0",
    "fine-uploader": "^5.12.0",
    "helmet": "~2.1.1",
    "jquery": "^2.2.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.7",
    "conventional-changelog-cli": "~1.2.0",
    "copy-webpack-plugin": "~3.0.1",
    "core-js": "~2.4.1",
    "cross-env": "~2.0.0",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~3.2.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.22.0",
    "immutable": "~3.8.1",
    "istanbul-instrumenter-loader": "~0.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "~1.1.2",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "morgan": "~1.7.0",
    "node-sass": "~3.8.0",
    "nodemon": "~1.10.0",
    "npm-run-all": "~2.3.0",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "~4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "~0.13.1",
    "ts-helpers": "~1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "~3.14.0",
    "typescript": "~1.8.10",
    "typings": "~1.3.2",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-md5-hash": "~0.0.5",
    "yargs": "~4.8.1",
    "zone.js": "~0.6.12"
  }
}
